odalDelete(id){\r\n        this.dialogDelete = true;\r\n        this.dataDeleteForm.id = id\r\n        this.dataDeleteForm.deletedAt = this.currentDate()\r\n    }\r\n    async deleteContact(){\r\n        const res = await contactModule.deleteContacto(this.FormRequestDelete);\r\n        this.desserts = res;\r\n        this.dialogDelete = false;\r\n    }\r\n    close(event){\r\n        this.dialog = false\r\n    }\r\n    handleData(event){\r\n        this.desserts = event;\r\n    }\r\n    currentDate() {\r\n        var date = new Date();\r\n        return  date.toISOString();\r\n    }\r\n\r\n    async getContactos() {\r\n        const res = await  contactModule.getContactosAll();\r\n        this.desserts = res;\r\n        this.loadTable = false;\r\n    }\r\n\r\n    async getClientesAll(){\r\n        const cliente = await  contactModule.getClientes();\r\n        this.clientes = cliente;\r\n        console.log(this.clientes)\r\n    }\r\n\r\n    mounted(){\r\n        this.getContactos();\r\n        this.getClientesAll();\r\n    }\r\n\r\n}\r\n"],"sourceRoot":""}]}