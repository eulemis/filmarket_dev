,\r\n    Action,\r\n    VuexModule,\r\n  } from 'vuex-module-decorators';\r\n  import store from '@/store';\r\n\r\n  \r\n  @Module({\r\n    namespaced: true,\r\n    name: 'Orders',\r\n    store,\r\n    dynamic: true,\r\n  })\r\n  \r\n  class orderModule extends VuexModule {\r\n    items: any[] = [];\r\n    //getters\r\n    get getCartList() {\r\n        return this.items\r\n    }\r\n\r\n    @Mutation\r\n    ADD_CART(data:any) {\r\n        let findId = this.items.find(product => product.id == data.id)\r\n        \r\n        if(findId) {\r\n            data.qty += 1\r\n        }else { \r\n            data.qty = 1\r\n            this.items.push(data) \r\n            localStorage.setItem('_dataSelect', JSON.stringify(this.items));        \r\n        }\r\n    }\r\n\r\n    @Action\r\n    addCart(data:any) {\r\n        this.ADD_CART(data)\r\n    }\r\n\r\n  }\r\n  export default getModule(orderModule);"],"sourceRoot":""}]}