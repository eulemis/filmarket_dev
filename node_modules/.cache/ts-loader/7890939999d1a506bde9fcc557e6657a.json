rm  = {\r\n        id : '',\r\n        deletedAt : ''\r\n    }\r\n   \r\n\r\n    get FormRequestDelete(): any {\r\n        return serialize(this.dataDeleteForm,'clientes',{});\r\n    }\r\n    openModal(){\r\n        this.tituloModal = 'Crear Cliente'\r\n        this.dialog = true\r\n    }\r\n    editData(item){\r\n        this.dataEditForm = item\r\n        this.tituloModal = 'Actualizar Cliente'\r\n        this.dialog = true\r\n    }\r\n    showModalDeleteClient(id){\r\n        this.dialogDelete = true;\r\n        this.dataDeleteForm.id = id\r\n        this.dataDeleteForm.deletedAt = this.currentDate()\r\n    }\r\n    async deleteClient(){\r\n        this.overlay = true\r\n        const res = await clientModule.deleteCliente(this.FormRequestDelete);\r\n        this.desserts = res;\r\n        this.dialogDelete = false;\r\n        this.overlay = false\r\n    }\r\n    close(event){\r\n        this.dialog = false\r\n    }\r\n    handleData(event){\r\n        this.desserts = event;\r\n    }\r\n    currentDate() {\r\n        var date = new Date();\r\n        return  date.toISOString();\r\n    }\r\n\r\n    async getClientes() {\r\n        const res = await  clientModule.getClientesAll();\r\n        this.desserts = res;\r\n        this.loadTable = false;\r\n    }\r\n\r\n    async comboboxAll(){\r\n        const zona              = await  clientModule.getZonasAll();\r\n        this.zonas = zona;\r\n        const tipocliente       = await  clientModule.getTipoClienteAll();\r\n        this.tipoclientes = tipocliente;\r\n        const segmento          = await  clientModule.getSegmentosAll();\r\n        this.segmentos = segmento;\r\n        const vendedor          = await  clientModule.getVendedorAll();\r\n        this.vendedor = vendedor;\r\n        const condicionespago   = await  clientModule.getCondicionesPagoAll();\r\n        this.condicionespago = condicionespago\r\n    }\r\n\r\n    mounted(){\r\n        this.getClientes();\r\n        this.comboboxAll();\r\n    }\r\n\r\n}\r\n"],"sourceRoot":""}]}