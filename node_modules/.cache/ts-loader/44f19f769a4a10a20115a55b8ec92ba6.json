o(event){\r\n        this.desserts = event;\r\n    }\r\n \r\n    currentDate() {\r\n        var date = new Date();\r\n        return  date.toISOString();\r\n    }\r\n\r\n    async getContactos() {\r\n        let contact : any = [];\r\n        const res : any = await  contactModule.getContactosAll();\r\n        this.pagination.current_page = res.data.meta.page.current_page,\r\n        this.pagination.from = res.data.meta.page.from,\r\n        this.pagination.last_page = res.data.meta.page.last_page,\r\n        this.pagination.per_page = res.data.meta.page.per_page,\r\n        this.pagination.to = res.data.meta.page.to,\r\n        this.pagination.total = res.data.meta.page.total\r\n        contact = deserialize(res.data)\r\n        //this.desserts = contact;\r\n        this.loadTable = false;\r\n    }\r\n/* \r\n    async getClientesAll(){\r\n        const cliente : any = await  contactModule.getClientes();\r\n        this.clientes = cliente;\r\n    } */\r\n\r\n    editarContacto(id_contacto) {\r\n        this.$router.push({ name: \"editarcontacto\", params: { id: id_contacto } });\r\n    }\r\n\r\n    mounted(){\r\n        this.getContactos();\r\n       // this.getClientesAll();\r\n    }\r\n\r\n}\r\n"],"sourceRoot":""}]}