age[size]=${data.itemsPerPage}&sort=-id`)\r\n                .then(response =>  {\r\n\r\n                        if (response.status === 200) {     \r\n                            resolve(response);\r\n                        }\r\n                })\r\n                .catch(error => {\r\n                    reject(error)\r\n                })\r\n            } \r\n         \r\n            }) \r\n\t     }\r\n\r\n  }  \r\n  \r\n  export default getModule(globalModule);\r\n  "],"sourceRoot":""}]}